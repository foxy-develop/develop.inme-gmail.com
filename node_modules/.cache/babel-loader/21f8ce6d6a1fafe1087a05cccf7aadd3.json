=\"app\" :class=\"setTheme\" :style=\"cssColorsVars\">\n    <div id=\"nav\">\n      <router-link to=\"/\">Home 2</router-link> |\n      <router-link to=\"/about\">About</router-link>\n    </div>\n    <router-view />\n  </div>\n</template>\n\n<script>\nimport { ThemeService } from './services/storage.service';\n\nexport default {\n  name: 'app',\n  metaInfo: {\n    title: 'ArtDock Client Panel'\n  },\n  data() {\n    return {\n      theme: ''\n    };\n  },\n  computed: {\n    setTheme: function() {\n      if (!ThemeService.get()) {\n          ThemeService.save('dark');\n      }\n      return ThemeService.get();\n    },\n    cssColorsVars() {\n      return {\n        // general\n        '--dark': this.$const.COLOR[this.theme.toUpperCase()].DARK,\n        '--dark-gray': this.$const.COLOR[this.theme.toUpperCase()].DARK_GRAY,\n        '--gray': this.$const.COLOR[this.theme.toUpperCase()].GRAY,\n        '--light-gray': this.$const.COLOR[this.theme.toUpperCase()].LIGHT_GRAY,\n        '--light': this.$const.COLOR[this.theme.toUpperCase()].LIGHT,\n        '--primary-color': this.$const.COLOR[this.theme.toUpperCase()].APP_PRIMARY,\n        '--secondary-color': this.$const.COLOR[this.theme.toUpperCase()].APP_SECONDARY,\n        '--error-color': this.$const.COLOR[this.theme.toUpperCase()].ERROR,\n      };\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n\n#nav {\n  padding: 30px;\n\n  a {\n    font-weight: bold;\n    color: #2c3e50;\n\n    &.router-link-exact-active {\n      color: #42b983;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src"}]}