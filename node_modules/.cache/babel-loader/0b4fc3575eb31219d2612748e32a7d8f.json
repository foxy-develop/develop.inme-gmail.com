AACA,SAAA,gBAAA,CAAA,KAAA,YAAA;AACA,GArBA;AAsBA,EAAA,OAAA,oBACA,UAAA,CAAA,CAAA,kBAAA,EAAA,aAAA,EAAA,YAAA,EAAA,iBAAA,CAAA,CADA;AAEA,IAAA,qBAFA,iCAEA,OAFA,EAEA;AACA,WAAA,eAAA,CAAA;AAAA,QAAA,SAAA,EAAA,OAAA,CAAA,EAAA;AAAA,QAAA,MAAA,EAAA,KAAA;AAAA,OAAA;AACA,WAAA,gBAAA,CAAA,KAAA,YAAA;AAEA,KANA;AAQA,IAAA,SARA,qBAQA,MARA,EAQA;AACA,WAAA,UAAA,CAAA,MAAA;AACA,WAAA,gBAAA,CAAA,MAAA;AACA,KAXA;AAaA,IAAA,YAbA,0BAaA;AACA,WAAA,QAAA,GAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,KAfA;AAiBA,IAAA,UAjBA,wBAiBA;AACA,UAAA,IAAA,GAAA,KAAA,UAAA,CAAA,KAAA,YAAA,CAAA;AACA,WAAA,YAAA,GAAA,IAAA,CAAA,MAAA,GAAA,CAAA;AACA,KApBA;AAqBA,IAAA,SArBA,qBAqBA,IArBA,EAqBA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,UAAA,EAAA,EAAA;AACA,eAAA;AACA,UAAA,EAAA,EAAA,EAAA,CAAA,SADA;AAEA,UAAA,KAAA,EAAA,EAAA,CAAA,IAFA;AAGA,UAAA,WAAA,EAAA,EAAA,CAAA,EAHA;AAIA,UAAA,KAAA,EAAA,EAAA,CAAA;AAJA,SAAA;AAMA,OAPA,EAOA,MAPA,CAOA,UAAA,EAAA;AAAA,eAAA,MAAA,CAAA,EAAA,CAAA,KAAA,CAAA;AAAA,OAPA,CAAA;AAQA;AA9BA,IAtBA;AAsDA,EAAA,QAAA,oBACA,UAAA,CAAA,CACA,kBADA,EAEA,aAFA,EAGA,YAHA,EAIA,cAJA,EAKA,UALA,EAMA,wBANA,EAOA,aAPA,EAQA,oBARA,CAAA,CADA,CAtDA;AAmEA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,SAAA,CAAA,KAAA,UAAA;AACA;AAHA,GAnEA;AAwEA,EAAA,OAxEA,qBAwEA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,QAAA,EAAA,KAAA,YAAA;AACA,SAAA,YAAA;AACA,GA3EA;AA4EA,EAAA,SA5EA,uBA4EA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,YAAA;AACA;AA9EA,CAAA","sourcesContent":["<template>\n  <div class=\"main__content\">\n    <Section :title=\"'Негативные упоминания'\" :navBtn=\"true\" isStart full isSideFilters>\n      <template v-slot:side>\n        <TotalCounter\n          :to=\"isNegativeLoaded ? getCount : 0\"\n          :type=\"'negative'\"\n        />\n        <transition-group tag=\"div\" appear class=\"negative__filter\" name=\"reveal-animate--right\" v-if=\"isMobile && isMapLoaded\">\n          <PeriodSwitcher\n            key=\"period\"\n            :current=\"getMapPeriod\"\n            :name=\"'map'\"\n            :callback=\"setPeriod\"\n          ></PeriodSwitcher>\n          <Dropdown\n            key=\"country\"\n            :options=\"countries(getMapData)\"\n            :type=\"'country'\"\n            num\n            :count=\"isNegativeLoaded ? getCount : 0\"\n            :selected=\"getSelectedCountry(getNegativeFilterState)\"\n            v-on:updateOption=\"updateSelectedCountry\"\n            :placeholder=\"'Страна'\"\n          />\n        </transition-group>\n      </template>\n      <template v-slot:control>\n        <PeriodSwitcher\n          v-if=\"!isMobile\"\n          :current=\"getMapPeriod\"\n          :name=\"'map'\"\n          :callback=\"setPeriod\"\n        ></PeriodSwitcher>\n      </template>\n      <template v-slot:content>\n        <BlockList :load=\"isNegativeLoaded\" load>\n          <List :load=\"isNegativeLoaded\"\n                :dataList=\"getNegative\"\n                :type=\"'negative'\"/>\n        </BlockList>\n      </template>\n    </Section>\n  </div>\n</template>\n\n<script>\n  import { mapActions, mapGetters } from \"vuex\";\n  import PeriodSwitcher from \"../components/period-swicther\";\n  import BlockList from \"../components/layout/BlockList\";\n  import TotalCounter from \"../components/counters/TotalCounter\";\n  import List from \"../components/list\";\n  import Section from \"../components/layout/Section\";\n  import Dropdown from \"../components/dropdown\";\n\n\n    export default {\n      name: \"Negative\",\n      components: {\n        Section, PeriodSwitcher, BlockList, List, TotalCounter, Dropdown\n      },\n      metaInfo: {\n        title: \"Негативные упоминания\",\n        titleTemplate: \"%s | ArtDock Client Panel\"\n      },\n      data() {\n        return {\n          isMobile: false,\n          region_search: \"\",\n          period: \"\",\n          isShowSearch: false,\n          regions: []\n        };\n      },\n      mounted() {\n        this.isMapLoaded || this.MAP_REQUEST();\n        this.NEGATIVE_REQUEST(this.getMapPeriod)\n      },\n      methods: {\n        ...mapActions([\"NEGATIVE_REQUEST\", \"MAP_REQUEST\", \"MAP_SWITCH\", \"NEGATIVE_FILTER\"]),\n        updateSelectedCountry(payload) {\n          this.NEGATIVE_FILTER({region_id: payload.id, period: this.getMapPeriod})\n          this.NEGATIVE_REQUEST(this.getMapPeriod);\n\n        },\n\n        setPeriod(period) {\n          this.MAP_SWITCH(period);\n          this.NEGATIVE_REQUEST(period);\n        },\n\n        handleResize() {\n          this.isMobile = window.innerWidth < 1024;\n        },\n\n        showSearch() {\n          const data = this.getMapData(this.getMapPeriod);\n          this.isShowSearch = data.length > 6;\n        },\n        countries(data) {\n          return data.map(el => {\n            return {\n              id: el.region_id,\n              title: el.name,\n              title_short: el.id,\n              value: el.value_negative\n            }\n          }).filter( el => Number(el.value) )\n        }\n      },\n      computed: {\n        ...mapGetters([\n          \"isNegativeLoaded\",\n          \"getNegative\",\n          \"getMapData\",\n          \"getMapPeriod\",\n          \"getCount\",\n          'getNegativeFilterState',\n          \"isMapLoaded\",\n          \"getSelectedCountry\"\n        ]),\n\n      },\n      watch: {\n        period() {\n          this.countries(this.getMapData)\n        }\n      },\n      created() {\n        window.addEventListener(\"resize\", this.handleResize);\n        this.handleResize();\n      },\n      destroyed() {\n        window.removeEventListener(\"resize\", this.handleResize);\n      },\n    }\n\n</script>\n\n<style scoped lang=\"scss\">\n  .negative {\n    &__filter {\n      display: flex;\n      width: 100%;\n      flex-direction: row;\n      height: 5rem;\n      border-radius: .8rem;\n      justify-content: space-between;\n      align-items: center;\n      position: relative;\n      padding-bottom: 1rem;\n\n      @include tablet {\n        padding-bottom: 0;\n        height: unset;\n        flex-direction: column;\n        max-width: 32.6rem;\n\n      }\n    }\n  }\n</style>\n"],"sourceRoot":"src/views"}]}