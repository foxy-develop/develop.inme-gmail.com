Root":"src/components/filters","sourcesContent":["<template>\n  <div class=\"filter\">\n    <div class=\"filter__item\">\n      <div class=\"filter__title\">\n        <Dropdown\n          v-if=\"isDataLoaded\"\n          :type=\"'keywords'\"\n          :options=\"getFilter.keywords\"\n          :selected=\"getFilterState('keywords')[0] || ''\"\n          v-on:updateOption=\"updateSelectedKeywords\"\n          :placeholder=\"'Ключевое слово'\"\n        >\n        </Dropdown>\n      </div>\n    </div>\n    <div class=\"filter__item\">\n      <div class=\"filter__title\">\n        <Dropdown\n          v-if=\"isDataLoaded\"\n          :options=\"getFilter.limit_options\"\n          :type=\"'top'\"\n          :selected=\"getFilterState('limit_options')[0]\"\n          v-on:updateOption=\"updateSelectedTop\"\n          :placeholder=\"'ТОП'\"\n        >\n        </Dropdown>\n      </div>\n    </div>\n    <div class=\"filter__item\">\n      <div class=\"filter__title\">\n        <Dropdown\n          v-if=\"isDataLoaded\"\n          :options=\"getFilter.countries\"\n          :type=\"'country'\"\n          :selected=\"getFilterState('countries')[0] || getFilter.countries[0]\"\n          v-on:updateOption=\"updateSelectedCountry\"\n          :placeholder=\"'Страна'\"\n        >\n        </Dropdown>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Dropdown from \"../dropdown\";\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n  name: \"MentionsFilter\",\n  components: {\n    Dropdown\n  },\n  data() {\n    return {\n      data: \"\",\n      per_page: \"\",\n      country: \"\",\n      keywords: \"\"\n    }\n  },\n  methods: {\n    ...mapActions([\"MENTIONS_CHANGE_FILTER\"]),\n    saveFilter() {\n      const options = {\n        keywords: this.keywords,\n        limit_options: this.per_page,\n        countries: this.country\n      };\n      this.MENTIONS_CHANGE_FILTER({ options });\n    },\n\n    updateSelectedCountry(payload) {\n      this.country = payload.id;\n    },\n\n    updateSelectedTop(payload) {\n      this.per_page = payload.id;\n    },\n\n    updateSelectedKeywords(payload) {\n      this.keywords = payload.id;\n    }\n  },\n  computed: {\n    ...mapGetters([\"getFilterState\", \"getFilter\", \"isDataLoaded\"]),\n    countryFlag() {\n      const stateFilter = this.getFilterState(\"countries\")[0];\n      if (stateFilter) {\n        return stateFilter.title_short.toLowerCase();\n      } else {\n        return this.getFilter.countries[0].title_short.toLowerCase();\n      }\n    }\n  },\n  watch: {\n    keywords: function() {\n      this.saveFilter();\n    },\n    per_page: function() {\n      this.saveFilter();\n    },\n    country: function() {\n      this.saveFilter();\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n  .filter {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: center;\n    box-sizing: border-box;\n    font-weight: 500;\n    font-size: 18px;\n    line-height: 20px;\n    font-family: \"Rubik\", sans-serif;\n    position: relative;\n    margin-bottom: 25px;\n    background: var(--el_bg);\n    box-shadow: 0px 1px 8px rgba(20, 46, 110, 0.1);\n    padding: 0;\n    border-radius: 8px;\n    transition: 0.3s ease-in-out;\n    z-index: 150;\n\n\n    @include tablet {\n      margin-bottom: 0;\n      box-shadow: none;\n      background: rgba(255, 255, 255, 0);\n      height: 135px;\n      justify-content: space-between;\n      padding: 0;\n    }\n\n\n    &__list {\n      position: absolute;\n      top: 100%;\n      width: 100%;\n      padding-left: 35px;\n      padding-right: 50px;\n      box-sizing: border-box;\n      margin: 0;\n      background: #fff;\n    }\n\n\n    &__item {\n      display: flex;\n      box-sizing: border-box;\n      height: 90px;\n      align-items: center;\n      justify-content: center;\n      border-radius: 8px;\n      cursor: pointer;\n      transition: 0.3s ease-in-out;\n      letter-spacing: 0.1px;\n      z-index: 102;\n      flex: 0 0 1;\n      max-width: calc(100% / 3);\n      background: var(--el_bg);\n\n      @include tablet {\n        padding: 0 25px;\n        box-shadow: 0px 1px 8px rgba(20, 46, 110, 0.1);\n        margin-right: 15px;\n        position: relative;\n        margin-bottom: 25px;\n        transform: translateX(30px);\n      }\n      @include desktop {\n        height: 110px;\n      }\n      @include desktop-lg {\n        padding: 0 35px;\n        margin-right: 2.5rem;\n      }\n      \n    }\n    &__icon {\n      margin-right: 15px;\n      flex-shrink: 0;\n      width: 4rem;\n      transition: 0.3s ease-in-out;\n      @include desktop-lg {\n        width: 40px;\n        margin-right: 30px;\n      }\n      &--flag {\n        font-size: 3rem;\n        border-radius: 0.8rem;\n        box-shadow: 0px 5px 13px rgba(20, 78, 110, 0.1);\n        width: 4rem;\n      }\n    }\n    &__chevron {\n      transition: 0.3s ease-in-out;\n    }\n    &__title {\n      width: 100%;\n      transition: 0.3s ease-in-out;\n      height: 100%;\n    }\n  }\n</style>\n"]}]}