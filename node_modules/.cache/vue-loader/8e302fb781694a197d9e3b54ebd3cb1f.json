iv class=\"empty-result\">По данному запросу ничего не найдено...</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: \"dropdown\",\n    data() {\n      return {\n        search: \"\",\n        selectedOption: {\n          title: \"\",\n          id: \"\",\n          title_short: \"\"\n        },\n        showMenu: false,\n        placeholderText: \"Пожалуйста сделайте выбор\"\n      };\n    },\n    props: {\n      options: {\n        type: [Array, Object]\n      },\n      type: String,\n      selected: {},\n      placeholder: [String],\n      closeOnOutsideClick: {\n        type: [Boolean],\n        default: true\n      }\n    },\n\n    mounted() {\n      this.selectedOption = this.selected;\n      if (this.placeholder) {\n        this.placeholderText = this.placeholder;\n      }\n\n      if (this.closeOnOutsideClick) {\n        document.addEventListener(\"click\", this.clickHandler);\n      }\n    },\n    beforeDestroy() {\n      document.removeEventListener(\"click\", this.clickHandler);\n    },\n\n    methods: {\n      updateOption(option) {\n        this.selectedOption = option;\n        this.showMenu = false;\n        this.search = \"\";\n        this.$emit(\"updateOption\", this.selectedOption);\n      },\n\n      toggleMenu() {\n        this.showMenu = !this.showMenu;\n      },\n\n      clickHandler(event) {\n        const { target } = event;\n        const { $el } = this;\n\n        if (!$el.contains(target)) {\n          this.showMenu = false;\n        }\n      },\n      getTitle: name => name.toLowerCase(),\n      filteredItems(arr) {\n        if (this.search) {\n          const condition = new RegExp(this.search, \"i\");\n          return arr.filter(item => item.title.match(condition));\n        }\n        return arr;\n      }\n    }\n  };\n</script>\n\n<style lang=\"scss\">\n  @keyframes showDrop {\n    from {\n      opacity: 0;\n      max-height: 0;\n      padding-bottom: 0;\n      padding-t