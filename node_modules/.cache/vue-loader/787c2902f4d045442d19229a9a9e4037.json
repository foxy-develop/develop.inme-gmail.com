>\n    </Section>\n  </div>\n</template>\n\n<script>\nimport Section from \"../components/layout/Section\";\nimport ChartCounter from \"../components/counters/ChartCounter\";\nimport Block from \"../components/layout/Block\";\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n  name: 'Dashboard',\n  components: {\n    Section, ChartCounter, Block\n  },\n  data() {\n    return {\n      preload: false\n    }\n  },\n  methods: {\n    ...mapActions(['DATA_REQUEST'])\n  },\n  async mounted() {\n    if (!this.isChartDataLoaded) {\n      this.DATA_REQUEST().then(() => {\n        this.preload = true;\n      });\n    } else {\n      this.preload = true;\n    }\n  },\n  computed: {\n    ...mapGetters(['getChartData', \"getChartFilters\", \"isChartDataLoaded\"]),\n    countersData: function() {\n      const data = this.getChartData;\n      return {\n        positive: data.positive.total,\n        negative: data.negative.total\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n\n</style>\n"]}]}